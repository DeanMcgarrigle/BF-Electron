{
  "name": "MachSecure.Launcher",
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "start": "Electron .",
    "watch": "./node_modules/.bin/webpack-dev-server",
    "electron-rebuild": "./node_modules/.bin/electron-rebuild app",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server --content-base --hot --inline",
    "package": "electron-packager ./ MachSecure.Launcher --platform=win32 --arch=ia32 --version=0.36.7 --ignore=node_modules --ignore=dist --ignore=installer --ignore=cache --ignore=gulpfile.js --ignore=webpack.config.js --ignore=msiconfig.json --out=./dist --prune=true --asar=false --overwrite --icon=./app/img/atom.ico",
    "deploy": "electron-builder ./dist/MachSecure.Launcher-win32-ia32 --platform=win --out=./dist --config=./msiconfig.json"
  },
  "devDependencies": {
    "angular": "*",
    "angular-animate": "^1.5.0",
