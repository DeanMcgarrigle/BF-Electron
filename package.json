    "loader-utils": "*",
    "ng-annotate-loader": "*",
    "node-sass": "*",
    "raw-loader": "*",
    "sass-loader": "*",
    "slick-carousel": "^1.5.9",
    "source-map": "*",
    "style-loader": "*",
    "tether": "*",
    "url-loader": "*",
    "webpack": "*",
    "webpack-dev-server": "*",
    "webpack-target-electron-renderer": "*"
