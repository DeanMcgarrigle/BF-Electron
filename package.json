    "babel": "*",
    "babel-core": "*",
    "babel-loader": "*",
    "babel-preset-es2015": "*",
    "css-loader": "*",
    "electron-builder": "^2.7.2",
    "electron-packager": "*",
    "electron-prebuilt": "*",
    "electron-rebuild": "*",
    "electron-toaster": "*",
    "extract-text-webpack-plugin": "*",
    "file-loader": "*",
    "font-awesome-webpack": "*",
    "gulp": "*",
    "gulp-atom-electron": "^1.4.0",
    "gulp-electron": "*",
    "gulp-run": "*",
    "gulp-symdest": "^1.0.0",
    "gulp-util": "*",
    "jshint": "*",
    "jshint-loader": "*",
    "json-loader": "*",
